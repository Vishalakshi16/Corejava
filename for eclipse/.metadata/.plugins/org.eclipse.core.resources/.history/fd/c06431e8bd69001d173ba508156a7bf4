package com.xworkz.encapsulationapp;

public class AnimalDTO {
private String  name;
private String color;

public void SetName(String nm) {
	this.name=nm;
}

 public String getName() {
	return name;
	
}
 
 public void setColor(String clr) {
	 this.color=clr;
 }
 
 public String getColor() {
	 return color;
 }


 
}
